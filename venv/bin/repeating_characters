#!/Users/gndweber/projects/repeating-characters/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'repeating-characters','console_scripts','repeating_characters'
__requires__ = 'repeating-characters'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('repeating-characters', 'console_scripts', 'repeating_characters')()
    )
